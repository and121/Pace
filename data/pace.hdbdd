        ID_CREDITO: Integer;
        key NUM_CUOTA:Integer;
        MON_TOTAL: Integer;
        SEGURO_DEGRA:String(1);
        DIA_PAGO:Integer;
        FECHA_VENCIMIENTO: String(50);
        
        //Datos Veh√≠culos
